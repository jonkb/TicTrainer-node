<!DOCTYPE html>
<html lang="en-us">
	<head>
		<title>NewTics Session Ended</title>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link rel="stylesheet" type="text/css" href="/stylesheets/bgStyle.css">
	</head>
	<body>
    <section class="page-header">
			<div class="header-navbar">
				<a href="/" class="logo"></a>
				<h1>NewTics Session Ended</h1>
				<div style="clear: both;"></div>
			</div>
    </section>
    <section class="main-content">
			<button class='bigBtn' id='new_ses'>Start Another Session</button><br>
			<a href='/'><button class='bigBtn'>Return to Home</button></a>
    </section>
		<script>
			var sent = false;
			//Change the URL bar
			var url = "/nt/nt-session-ended.dynh";
			window.history.pushState("", "NewTics Session Ended", url);
			
			document.getElementById('new_ses').addEventListener('click', new_ses);
			
			function new_ses(){
				if(!sent){
					var xhr = new XMLHttpRequest();
					var reqBody = 'id=**[id]**&pw=**[pw]**&lid=**[lid]**';
					xhr.onreadystatechange = function(){
						if(xhr.readyState == 4){
							var response = xhr.responseText;
							document.open();
							document.write(response);
							document.close();
						}
					};
					xhr.open('POST', url, true);
					xhr.send(reqBody);
					sent = true;
				}
			}
		</script>
	</body>
</html>